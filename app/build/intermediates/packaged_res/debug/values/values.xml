    <style name="Vault.Text" parent="TextAppearance.Material3.BodyMedium">
        <item name="android:textColor">@color/vault_parchment</item>
    </style>
    <style name="Vault.Text.Amount">
        <item name="android:fontFamily">monospace</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/vault_parchment</item>
        <item name="android:letterSpacing">-0.01</item>
    </style>
    <style name="Vault.Text.Body">
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/on_surface_variant</item>
    </style>
    <style name="Vault.Text.Caption">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/text_hint</item>
        <item name="android:letterSpacing">0.05</item>
    </style>
    <style name="Vault.Text.Display">
        <item name="android:fontFamily">serif</item>
        <item name="android:textSize">28sp</item>
        <item name="android:textColor">@color/vault_parchment</item>
        <item name="android:letterSpacing">-0.02</item>
    </style>
    <style name="Vault.Text.SectionLabel">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0.15</item>
        <item name="android:textColor">@color/vault_brass_dim</item>
    </style>
    <style name="Vault.Text.ToolbarTitle">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:letterSpacing">0.15</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="Widget.Vault.Button" parent="Widget.Material3.Button">
        <item name="backgroundTint">@color/primary</item>
        <item name="android:textColor">@color/on_primary</item>
        <item name="cornerRadius">4dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0.1</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textStyle">bold</item>
        <item name="elevation">0dp</item>
        <item name="android:stateListAnimator">@null</item>
    </style>
    <style name="Widget.Vault.CardView" parent="Widget.Material3.CardView.Outlined">
        <item name="cardBackgroundColor">@color/surface_variant</item>
        <item name="cardElevation">0dp</item>
        <item name="cardCornerRadius">4dp</item>
        <item name="strokeWidth">1dp</item>
        <item name="strokeColor">@color/vault_border</item>
        <item name="contentPadding">0dp</item>
    </style>
    <style name="Widget.Vault.Chip" parent="Widget.Material3.Chip.Filter">
        <item name="chipBackgroundColor">@color/primary_container</item>
        <item name="android:textColor">@color/on_primary_container</item>
        <item name="chipCornerRadius">4dp</item>
        <item name="chipStrokeColor">@color/vault_border_warm</item>
        <item name="chipStrokeWidth">1dp</item>
    </style>
    <style name="Widget.Vault.TextInputLayout" parent="Widget.Material3.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/primary</item>
        <item name="boxStrokeWidthFocused">1dp</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="hintTextColor">@color/text_secondary</item>
        <item name="boxCornerRadiusTopStart">4dp</item>
        <item name="boxCornerRadiusTopEnd">4dp</item>
        <item name="boxCornerRadiusBottomStart">4dp</item>
        <item name="boxCornerRadiusBottomEnd">4dp</item>
    </style>
    <style name="Widget.Vault.Toolbar" parent="Widget.Material3.Toolbar">
        <item name="android:background">@color/background</item>
        <item name="titleTextColor">@color/vault_parchment</item>
        <item name="titleTextAppearance">@style/Vault.Text.ToolbarTitle</item>
    </style>
</resources>